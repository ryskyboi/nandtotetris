class Main {

    /** Initializes a Pong game and starts running it. */
    function void main() {
        var Frog frog;
        let frog = Frog.new(200);
        do Sys.wait(1000);
        do frog.move(20, 0);
        do Sys.wait(1000);
        do frog.move(200, 0);
        do Sys.wait(1000);
        do frog.move(200, 0);
        do Sys.wait(1000);
        do frog.move(200, 0);
        do Sys.wait(1000);
        do frog.move(200, 0);
        return;
    }
}
